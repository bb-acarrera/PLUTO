  "bugs": {
    "url": "https://github.com/unchartedsoftware/PLUTO/issues"
  },
  "homepage": "https://github.com/unchartedsoftware/PLUTO#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "commander": "^2.9.0",
    "csv": "^1.1.1",
    "express": "^4.15.2",
    "fs-extra": "^3.0.1",
    "rimraf": "^2.6.1"
  },
  "devDependencies": {
    "jsdoc": "^3.4.3",
    "qunitjs": "^2.3.2"
  }
