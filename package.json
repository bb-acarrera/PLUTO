{
  "name": "pluto",
  "version": "1.0.10",
  "description": "A data validation framework.",
  "main": "server.js",
  "scripts": {
    "start": "cd src && node server/server.js -s server/debugServerConfig.json -v runtime/configs/validatorConfig.json",
    "test": "sh build/bin/runUnitTests.sh",
    "test_ci": "sh build/bin/runUnitTestsCI.sh",
    "docker_build": "docker build -t pluto:develop ./Release/server",
    "docker_run_bash": "sh startPlutoInteractive.sh",
    "docker_save": "docker save --output pluto.image.tar pluto:develop",
    "build": "sh build/bin/build.sh",
    "start_debug": "cd src && node --inspect=9229 server/server.js -s server/debugServerConfig.json -v runtime/configs/validatorConfig.json",
    "start_docker": "sh startPluto.sh",
    "clean_devdb": "sh src/clean_dev_db.sh",
    "build_docs": "sh build/bin/buildDocs.sh",
    "load_devdata": "node database/dbloader/importRulesets.js -v src/runtime/configs/validatorConfig.json -r src/runtime/rulesets",
    "tag_images": "sh build/bin/tagImages.sh",
    "start_dev_container": "docker build -t pluto_dev -f dev_container/dev.Dockerfile . && docker run -v $PWD/.:/code -p 3000:3000 -p9229:9229 --net=plutonet --rm -a stdout -a stderr pluto_dev node --inspect src/server/server.js -s src/server/debugServerConfigContainer.json -v src/runtime/configs/validatorConfigContainer.json",
    "start_worker": "cd src && node validator/queueWorker.js -v runtime/configs/validatorConfig.json",
    "start_worker_debug": "cd src && node --inspect=9229 validator/queueWorker.js -v runtime/configs/validatorConfig.json"
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unchartedsoftware/PLUTO.git"
  },
  "author": "Uncharted Software",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/unchartedsoftware/PLUTO/issues"
  },
  "homepage": "https://github.com/unchartedsoftware/PLUTO#readme",
  "dependencies": {
    "adm-zip": "^0.4.7",
    "amqplib": "^0.5.2",
    "archiver": "^2.1.1",
    "aws-sdk": "^2.165.0",
    "body-parser": "^1.17.2",
    "bottleneck": "^1.16.0",
    "commander": "^2.9.0",
    "csv": "^1.1.1",
    "express": "^4.15.2",
    "express-fileupload": "^0.2.0",
    "express-history-api-fallback": "^2.2.1",
    "fs-extra": "^3.0.1",
    "gdal": "^0.9.6",
    "lodash": "^4.17.5",
    "md5-file": "^3.2.3",
    "moment": "^2.18.1",
    "nodemailer": "^4.4.0",
    "pg": "^7.0",
    "request": "^2.83.0",
    "rimraf": "^2.6.1",
    "stream-to-promise": "^2.2.0",
    "tree-kill": "^1.2.0"
  },
  "devDependencies": {
    "jsdoc": "^3.4.3",
    "karma": "^1.7.0",
    "karma-qunit": "^1.2.1",
    "markdown-to-html": "0.0.13",
    "node-pg-migrate": "^2.11.1",
    "proxyquire": "^1.8.0",
    "qunitjs": "2.4.1",
    "stack-trace": "0.0.10",
    "supervisor": "^0.12.0"
  }
}
