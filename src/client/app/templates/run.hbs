<div>
	{{#link-to 'runs' tagName='button' class='btn btn-success btn-sm btn-return-title'}}
		{{#bs-tooltip placement="left"}}Return to runs{{/bs-tooltip}}
        <i class="fa fa-angle-double-left"></i>
	{{/link-to}}

	<h3>
		{{#if model.ruleset.source}}
			{{model.source.group}} {{model.source.description}} {{model.ruleset.source.config.file}}
		{{else if model.ruleset.importer}}
			{{model.run.inputfilename}} from {{model.ruleset.importer.filename}}
		{{else}}
			{{model.run.inputfilename}}
		{{/if}}
	</h3>

</div>
<div>
	{{run-details model=model type=type page=page ruleid=ruleid}}
</div>
<div>
	{{#if model.ruleset}}
		{{#if (not model.ruleset.deleted)}}
			{{#link-to 'editRuleset' model.ruleset.filename}}Edit Current Ruleset{{/link-to}}
		{{/if}}
	{{/if}}
</div>

{{outlet}}
