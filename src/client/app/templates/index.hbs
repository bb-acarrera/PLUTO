{{outlet}}
<div>
	<h2>PLUTO Main Page</h2>
	<table id="mainPageTable" class="main-page-table">

		{{#if applicationController.currentUser.group}}
            <tr><td>{{#link-to "runs" (query-params runGroupFilter=applicationController.currentUser.group)}}File Load Status{{/link-to}}</td></tr>
            <tr><td>{{#link-to "rulesets" (query-params rulesetGroupFilter=applicationController.currentUser.group)}}Edit Validation Setups{{/link-to}}</td></tr>
		{{else}}
            <tr><td>{{#link-to "runs" }}File Load Status{{/link-to}}</td></tr>
            <tr><td>{{#link-to "rulesets" }}Edit Validation Setups{{/link-to}}</td></tr>
		{{/if}}



        <!--
        <tr><td>Edit Rule Groups</td></tr>
        <tr><td>Edit Sources and Targets</td></tr>
        -->
	</table>
	{{#bs-button onClick=(action "setShowAddRuleset")}}
        Add new file to load
	{{/bs-button}}
</div>

{{add-ruleset showdialog=showAddRuleset parsers=model.parsers
	defaultSources=model.defaultSources defaultTargets=model.defaultTargets}}

